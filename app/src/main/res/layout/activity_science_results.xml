<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    tools:context="com.example.genesissy.thabeloquiz.ScienceResults">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >


            <TextView
                android:id="@+id/textView23"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Results Page"
                android:textSize="49sp" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TOTAL SCORE"
                android:textSize="40dp"
                android:gravity="center"
                android:layout_marginTop="10dp"/>
            <TextView
                android:id="@+id/total"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0"
                android:textSize="40sp"
                android:gravity="center"/>

            <TextView
                android:layout_marginTop="0dp"
                android:id="@+id/Timer_Head"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Timer"
                android:layout_gravity="center_horizontal"
                android:textSize="55dp"
                />

            <TextView
                android:id="@+id/Timer_Resever"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/Timer_Head"
                android:text="0:00"
                android:textStyle="bold"
                android:textColor="#7FFF00"
                android:textSize="65dp" />

            <RadioGroup
                android:id="@+id/quetion_One"
                android:layout_marginTop="45dp"
                android:layout_below="@id/Timer_Head"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:gravity="center"
                    android:id="@+id/textView2"
                    android:textColor="#000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Electric current may be expressed in which one of the following
    units?"
                    android:textSize="26sp" />


                <RadioButton
                    android:id="@+id/radioButton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Coulombs/second"
                    android:textSize="26sp" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="19dp"
                android:id="@+id/quetion_Two"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/quetion_One"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:textColor="#000"
                    android:gravity="center"
                    android:layout_marginBottom="10dp"
                    android:text="In the SI system of measure, what is the unit of capacitance?" />

                <RadioButton
                    android:textSize="26sp"
                    android:id="@+id/radioButton6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Kilogram-meter per second squared" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:layout_below="@id/quetion_Two"
                android:id="@+id/Question_Three"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:textColor="#000"
                    android:gravity="center"
                    android:layout_marginBottom="15dp"
                    android:text="Work is equal to which of the following?" />


                <RadioButton
                    android:id="@+id/radioButton8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="the dot product of force and displacement" />


            </RadioGroup>

            <RadioGroup
                android:id="@+id/Question_Four"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="15dp">

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginBottom="15dp"
                    android:text="The work done by a friction force is:"
                    android:textColor="#000"
                    android:textSize="26sp" />

                <RadioButton
                    android:id="@+id/radioButton12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Always Negetive"
                    android:textSize="26sp" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Five"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:gravity="center"
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:textColor="#000"
                    android:text="As defined in physics, work is" />

                <RadioButton
                    android:id="@+id/radioButton15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="a scalar quantity" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Six"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                <TextView
                    android:textColor="#000"
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="26sp"
                    android:text="A block of metal which weighs 60 newtons in air and 40 newtons
under water has a density, in kilograms per meter cubed, of:" />


                <RadioButton
                    android:id="@+id/radioButton16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="3000" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Seven"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                <TextView
                    android:id="@+id/textView5"
                    android:textColor="#000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:gravity="center"
                    android:layout_marginBottom="15dp"
                    android:text="If the distance between two objects, each of mass 'M', is tripled, the
force of attraction between the two objects is:" />

                <RadioButton
                    android:id="@+id/radioButton21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="1/9 the original force" />



            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Questio_Eight"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >



                <TextView
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:gravity="center"
                    android:textColor="#000"
                    android:text="In physics, a radian per second is a unit of:" />


                <RadioButton
                    android:id="@+id/radioButton22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="angular velocity" />

            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Nine"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:gravity="center"
                    android:textColor="#000"
                    android:layout_marginBottom="15dp"
                    android:text="If the resultant force acting on a body of constant mass is zero, the
body's momentum is" />

                <RadioButton
                    android:id="@+id/radioButton28"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="Constant" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Ten"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:gravity="center"
                    android:textColor="#000"
                    android:text="The force acting between two point charges can be computed using
which of the following laws?" />

                <RadioButton
                    android:id="@+id/radioButton29"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="Coulomb's Law" />
            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Eleven"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <RadioGroup
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <TextView
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:gravity="center"
                    android:textColor="#000"
                    android:text="Induced electric currents can be explained using which of the
following laws?" />

                <RadioButton
                    android:id="@+id/radioButton34"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="Faraday's Law" />

            </RadioGroup>

            <RadioGroup
                android:id="@+id/Question_Thwelf"
                android:layout_marginTop="15dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:textColor="#000"
                    android:gravity="center"
                    android:text="An infinitely long wire carries a current of three amps. The magnetic
field outside the wire:" />

                <RadioButton
                    android:id="@+id/radioButton35"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="circles the wire" />
            </RadioGroup>

            <RadioGroup
                android:id="@+id/Thirteen"
                android:layout_marginTop="15dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:textColor="#000"
                    android:gravity="center"
                    android:text="Iron is what type of magnetic material? Is it" />

                <RadioButton
                    android:id="@+id/radioButton40"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="ferromagnetic" />

            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Fourteen"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >


                <TextView
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:textColor="#000"
                    android:gravity="center"
                    android:text="A standing wave is formed on a tightly stretched string. The
distance between a node and an antinode is:" />


                <RadioButton
                    android:id="@+id/radioButton42"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="1/4 wavelength" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_FiveTeen"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:gravity="center"
                    android:textColor="#000"
                    android:text="When a physical property such as charge exists in discrete
rather than in continuous amounts, the property is said to be:" />



                <RadioButton
                    android:id="@+id/radioButton45"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="quantized" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_SixTeen"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView17"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:gravity="center"
                    android:textColor="#000"
                    android:text="A virtual image can be formed by one or more of the following single
mirrors? Identify them." />


                <RadioButton
                    android:id="@+id/radioButton47"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="all of the above" />
            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_SevenTeen"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:gravity="center"
                    android:id="@+id/textView18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:textColor="#000"
                    android:text="Whose principle or law states that each point on a wavefront may be
considered a new wave source? Is it:" />

                <RadioButton
                    android:id="@+id/radioButton52"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="Huygen's Principle" />

            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_EigTeen"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:textColor="#000"
                    android:gravity="center"
                    android:id="@+id/textView19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="The wave nature of light is demonstrated by which of the following?" />



                <RadioButton
                    android:id="@+id/radioButton54"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="diffraction" />


            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_NineTeen"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:gravity="center"
                    android:textColor="#000"
                    android:layout_marginBottom="15dp"
                    android:id="@+id/textView20"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="The collision between a photon and a free electron was first
explained by which of the following scientists?" />


                <RadioButton
                    android:id="@+id/radioButton56"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="Compton" />
            </RadioGroup>

            <RadioGroup
                android:layout_marginTop="15dp"
                android:id="@+id/Question_Twenty"
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:gravity="center"
                    android:textSize="26sp"
                    android:id="@+id/textView21"
                    android:textColor="#000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="The Tesla and the Gauss are units of measure of:" />

                <RadioButton
                    android:id="@+id/radioButton61"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    android:text="magnetic field strength" />

            </RadioGroup>

        </LinearLayout>

    </ScrollView>


</RelativeLayout>
